<p id="notice"><%= notice %></p>

<h1>Reports</h1>

<table>
  <thead>
    <tr>
      <th>Doctor</th>
      <th>User</th>
      <th>Pdate</th>
      <th>Ptime</th>
      <th>Notes</th>
      <th>Prescription</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @reports.each do |report| %>
      <tr>
        <td><%= report.doctor %></td>
        <td><%= report.user %></td>
        <td><%= report.pdate %></td>
        <td><%= report.ptime %></td>
        <td><%= report.notes %></td>
        <td><%= report.prescription %></td>
        <td><%= link_to 'Show', report %></td>
        <td><%= link_to 'Edit', edit_report_path(report) %></td>
        <td><%= link_to 'Destroy', report, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Report', new_report_path %>
